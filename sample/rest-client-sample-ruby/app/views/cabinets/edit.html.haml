= form_tag cabinet_path(:id => @rest_cabinet.id), :method => :put do
  %p
    = label_tag :cabinet_name, 'Name'
    = text_field_tag :cabinet_name, @rest_cabinet.properties['object_name'], {:class => 'text_input'}
    = hidden_field_tag :page, params[:page]
  %p
    = submit_tag 'Save'
    = link_to '-Back-', :back